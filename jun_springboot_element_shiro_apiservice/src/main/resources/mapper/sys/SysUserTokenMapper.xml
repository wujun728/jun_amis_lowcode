<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.element.modules.sys.mapper.SysUserTokenMapper">

    <select id="getTokenInfo" resultType="com.element.modules.sys.entity.SysUserTokenEntity">
        SELECT
            *
        FROM
            sys_user_token
        WHERE
            token = #{value}
    </select>

</mapper>